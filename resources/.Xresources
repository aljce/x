! urxvt {{{
URxvt.internalBorder:  10
URxvt.loginShell:      true
! URxvt.cursorUnderline: true
! }}}

! globals {{{
*.scrollBar: false
! }}}

! fonts {{{
URxvt.font:     xft:Source Code Pro:size=20
URxvt.boldFont: xft:Source Code Pro:bold:size=20
! }}}

! colors {{{
! special
*.foreground:   #b2b2b2
*.background:   #292b2e
*.cursorColor:  #444155

! black
*.color0:       #222226
*.color8:       #5d4d7a

! red
*.color1:       #e0211d
*.color9:       #ce537a

! green
*.color2:       #4f97d7
*.color10:      #2d9574

! yellow
*.color3:       #9f8766
*.color11:      #b1951d

! blue
*.color4:       #4f97d7
*.color12:      #293239

! orange
*.color5:       #28def0
*.color13:      #665064

! cyan
*.color6:       #3c5a5d
*.color14:      #548084

! white
*.color7:       #7b8695
*.color15:      #94a1b3
! }}}
